# Kconfig T5UID1 settings for Atmel SAMD processors

config T5UID1_SERIAL
    bool "Enable DGUS T5UID1 screen"
    default n

choice
    depends on T5UID1_SERIAL
    prompt "Screen Serial Port" if LOW_LEVEL_OPTIONS
    help
        Select the serial device to use for the touchscreen.
    config ATSAMD_T5UID1_SERIAL_SERCOM0
        bool "SERCOM0" if !ATSAMD_SERIAL_SERCOM0
    config ATSAMD_T5UID1_SERIAL_SERCOM1
        bool "SERCOM1" if !ATSAMD_SERIAL_SERCOM1
    config ATSAMD_T5UID1_SERIAL_SERCOM2
        bool "SERCOM2" if !ATSAMD_SERIAL_SERCOM2
    config ATSAMD_T5UID1_SERIAL_SERCOM3
        bool "SERCOM3" if !ATSAMD_SERIAL_SERCOM3
    config ATSAMD_T5UID1_SERIAL_SERCOM4
        bool "SERCOM4" if !ATSAMD_SERIAL_SERCOM4 && (MACH_SAMD51 || MACH_SAMD21G18)
    config ATSAMD_T5UID1_SERIAL_SERCOM5
        bool "SERCOM5" if !ATSAMD_SERIAL_SERCOM5 && (MACH_SAMD51 || MACH_SAMD21G18)
    config ATSAMD_T5UID1_SERIAL_SERCOM6
        bool "SERCOM6" if !ATSAMD_SERIAL_SERCOM6 && (MACH_SAMD51N19 || MACH_SAMD51P20)
    config ATSAMD_T5UID1_SERIAL_SERCOM7
        bool "SERCOM7" if !ATSAMD_SERIAL_SERCOM7 && (MACH_SAMD51N19 || MACH_SAMD51P20)
endchoice

